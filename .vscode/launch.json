  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--target",
        "lib/main.dart",
        "--dart-define",
        "FLAVOR=production",
        "--dart-define-from-file",
        ".env.production"
      ]
    }
  ]
